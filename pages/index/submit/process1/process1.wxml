<!--pages/index/submit/process1/process1.wxml-->
<van-tabs active="{{ active }}">
  <van-tab title="飞行类型"></van-tab>
  <van-tab title="飞行日期"></van-tab>
  <van-tab title="飞行位置"></van-tab>
  <van-tab title="任务描述"></van-tab>
  <van-tab title="订单预算"></van-tab>
  <van-tab title="联系方式"></van-tab>
</van-tabs>

<view class="card">
  <!-- 2.飞行起始时间 -->
  <view style="margin-bottom: 20px;margin-top: 10px;">飞行开始</view>
  <!-- 具体时间 -->
  <picker mode="multiSelector" bindchange="bindMultiPickerChange" bindcolumnchange="bindMultiPickerColumnChange" value="{{multiIndex}}" range="{{multiArray}}" style="margin-bottom: 20px;">
      <input value='{{time}}' placeholder='选择时间'/>
  </picker>





  <view style="margin-bottom: 20px;">飞行结束</view>
  <!-- 具体时间 -->
  <picker mode="multiSelector" bindchange="bindMultiPickerChange" bindcolumnchange="bindMultiPickerColumnChange" value="{{multiIndex}}" range="{{multiArray}}"style="margin-bottom: 20px;">
      <input value='{{time}}' placeholder='选择时间'/>
  </picker>


  <!-- <button bindtap="gotoPageSubmit" type="primary" class="index-submit-process1"><text>上一步</text></button> -->
  <button bindtap="gotoPageProcess2" class="index-submit-process3"><text>下一步</text></button>
</view>
